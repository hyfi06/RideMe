{
  "name": "ride-me-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@ridemeapi-db-user",
    "DB_PASSWORD": "@ridemeapi-db-password",
    "DB_HOST": "@ridemeapi-db-host",
    "DB_NAME": "@ridemeapi-db-name",
    "API_KEY_MAPS": "@ridemeapi-api-key-maps",
    "AUTH_JWT_SECRET": "@ridemeapi-auth-jwt-secret"
  }
}
